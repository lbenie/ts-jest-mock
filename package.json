{
  "name": "ts-jest-mock",
  "version": "0.0.1",
  "description": "Maps to the proper jest mock type",
  "repository": "https://github.com/lbenie/ts-jest-mock.git",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "keywords": [
    "jest",
    "mock",
    "mocks",
    "unit",
    "unit test",
    "ut",
    "test",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest,stryker 'jest --coverage' 'stryker run'",
    "test:mutation": "stryker run",
    "format": "prettier lib --write",
    "build": "rollup -c"
  },
  "peerDependencies": {
    "@types/jest": "^26.0.21",
    "jest": "^26.6.3 || ^27.0.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@stryker-mutator/core": "5.0.0",
    "@stryker-mutator/jest-runner": "5.0.0",
    "@types/jest": "26.0.23",
    "concurrently": "6.2.0",
    "jest": "27.0.1",
    "jest-chain": "1.1.5",
    "jest-extended": "0.11.5",
    "prettier": "2.3.0",
    "rollup": "2.50.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-dts": "3.0.2",
    "ts-jest": "27.0.1",
    "tslib": "2.2.0",
    "typescript": "4.3.2"
  }
}
