{
  "name": "ts-jest-mock",
  "version": "0.0.1",
  "description": "Maps to the proper jest mock type",
  "repository": "https://github.com/lbenie/ts-jest-mock.git",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "keywords": [
    "jest",
    "mock",
    "mocks",
    "unit",
    "unit test",
    "ut",
    "test",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/ts-jest-mock.umd.js",
  "module": "./dist/ts-jest-mock.es.js",
  "exports": {
    ".": {
      "import": "./dist/ts-jest-mock.es.js",
      "require": "./dist/ts-jest-mock.umd.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest,stryker 'jest --coverage' 'stryker run'",
    "test:mutation": "stryker run",
    "format": "prettier lib --write",
    "build": "vite build"
  },
  "peerDependencies": {
    "@types/jest": "^26.0.21 || ^27.0.0",
    "jest": "^26.6.3 || ^27.0.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.2.5",
    "@stryker-mutator/core": "5.4.0",
    "@stryker-mutator/jest-runner": "5.4.0",
    "@types/jest": "27.0.1",
    "concurrently": "6.2.1",
    "jest": "27.2.0",
    "jest-chain": "1.1.5",
    "jest-extended": "0.11.5",
    "prettier": "2.4.1",
    "rollup": "2.56.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "ts-jest": "27.0.5",
    "tslib": "2.3.1",
    "typescript": "4.4.3",
    "vite": "2.5.10"
  }
}
