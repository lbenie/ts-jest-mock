{
  "name": "ts-jest-mock",
  "version": "1.3.0",
  "description": "Maps to the proper jest mock type",
  "repository": "https://github.com/lbenie/ts-jest-mock.git",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "keywords": [
    "jest",
    "mock",
    "mocks",
    "unit",
    "unit test",
    "ut",
    "test",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/ts-jest-mock.umd.cjs",
  "module": "./dist/ts-jest-mock.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/ts-jest-mock.js",
      "require": "./dist/ts-jest-mock.umd.cjs"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest 'jest --coverage'",
    "test:mutation": "stryker run",
    "format": "eslint lib --fix",
    "semantic-release": "npx semantic-release",
    "build": "vite build"
  },
  "devDependencies": {
    "@lbenie/linting": "1.8.58",
    "@rollup/plugin-typescript": "12.1.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@stryker-mutator/core": "8.7.1",
    "@stryker-mutator/jest-runner": "8.7.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "all-contributors-cli": "6.26.1",
    "concurrently": "9.1.0",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "jest": "29.7.0",
    "jest-chain": "1.1.6",
    "jest-extended": "4.0.2",
    "rollup": "4.28.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "semantic-release": "24.2.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsutils": "3.21.0",
    "typescript": "5.7.2",
    "vite": "6.0.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
