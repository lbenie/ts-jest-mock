{
  "name": "ts-jest-mock",
  "version": "1.3.0",
  "description": "Maps to the proper jest mock type",
  "repository": "https://github.com/lbenie/ts-jest-mock.git",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "keywords": [
    "jest",
    "mock",
    "mocks",
    "unit",
    "unit test",
    "ut",
    "test",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/ts-jest-mock.umd.cjs",
  "module": "./dist/ts-jest-mock.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/ts-jest-mock.js",
      "require": "./dist/ts-jest-mock.umd.cjs"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": "^20 || ^18"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest 'jest --coverage'",
    "test:mutation": "stryker run",
    "format": "eslint lib --fix",
    "build": "vite build"
  },
  "devDependencies": {
    "@lbenie/linting": "1.7.0",
    "@rollup/plugin-typescript": "11.1.6",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.0.2",
    "@stryker-mutator/core": "8.2.6",
    "@stryker-mutator/jest-runner": "8.2.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "all-contributors-cli": "6.26.1",
    "concurrently": "8.2.2",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "jest": "29.7.0",
    "jest-chain": "1.1.6",
    "jest-extended": "4.0.2",
    "rollup": "4.13.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "semantic-release": "23.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "tsutils": "3.21.0",
    "typescript": "5.4.2",
    "vite": "5.1.6"
  }
}
