{
  "name": "ts-jest-mock",
  "version": "1.3.0",
  "description": "Maps to the proper jest mock type",
  "repository": "https://github.com/lbenie/ts-jest-mock.git",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "keywords": [
    "jest",
    "mock",
    "mocks",
    "unit",
    "unit test",
    "ut",
    "test",
    "ts",
    "typescript"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "./dist/ts-jest-mock.umd.js",
  "module": "./dist/ts-jest-mock.es.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/ts-jest-mock.es.js",
      "require": "./dist/ts-jest-mock.umd.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": "^20 || ^18 || ^16"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n jest 'jest --coverage'",
    "test:mutation": "stryker run",
    "format": "eslint lib --fix",
    "build": "vite build"
  },
  "peerDependencies": {
    "@types/jest": "29.5.5",
    "jest": "29.7.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.7",
    "@stryker-mutator/core": "7.1.1",
    "@stryker-mutator/jest-runner": "7.1.1",
    "@types/jest": "29.5.5",
    "@types/node": "20.6.5",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "all-contributors-cli": "6.26.1",
    "concurrently": "8.2.1",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.7.0",
    "jest-chain": "1.1.6",
    "jest-extended": "4.0.1",
    "prettier": "3.0.3",
    "rollup": "3.29.3",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "semantic-release": "22.0.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "tsutils": "3.21.0",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}
